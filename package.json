{
	"name": "vbe-client-min",
	"displayName": "vbecm",
	"description": "excel vba module export, import, commit etc.",
	"publisher": "toramameseven",
	"author": {
		"name": "toramame-seven"
	},
	"version": "0.0.4",
	"icon": "images/logo.png",
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.69.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/*.xlsm",
		"workspaceContains:**/*.xlam",
		"workspaceContains:**/*.cls",
		"workspaceContains:**/*.bas",
		"workspaceContains:**/*.frm",
    "onView:vbeDiffView"
	],
	"main": "./out/extension.js",
	"contributes": {
    "views": {
      "explorer": [
        {
          "id": "vbeDiffView",
          "name": "Vbe Diff"
        }
      ]
    },
		"commands": [
			{
				"command": "explorer.export",
				"title": "Export all VBA modules"
			},
			{
				"command": "explorer.import",
				"title": "Import all VBA modules"
			},
			{
				"command": "explorer.compile",
				"title": "Compile VBA"
			},
			{
				"command": "explorer.exportFrx",
				"title": "Export only frx modules"
			},
      {
				"command": "explorer.commit-all",
				"title": "Push All Vba modules"
			},
      {
				"command": "explorer.testModified",
				"title": "check modified"
			},
			{
				"command": "editor.run",
				"title": "Run Sub function"
			},
			{
				"command": "editor.commit",
				"title": "Push VBA module"
			},
			{
				"command": "editor.checkout",
				"title": "Pull VBA module"
			},
      {
				"command": "vbeDiffView.refreshEntry",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
      {
				"command": "vbeDiffView.collapseAll",
				"title": "collapse All",
				"icon": "$(collapse-all)"
			},
      {
				"command": "vbeDiffView.diffVbeSrc",
				"title": "Diff vbe to src"
			},
      {
				"command": "vbeDiffView.pushSrc",
				"title": "Push"
			},
      {
				"command": "vbeDiffView.resolveVbe",
				"title": "Resolve vbe"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceExtname =~ /\\.xlsm$|\\.xlam$/i && vbecm.showVbsCommand && !explorerResourceIsFolder",
					"command": "explorer.export",
					"group": "myGroup@1"
				},
				{
					"when": "resourceExtname =~ /\\.xlsm$|\\.xlam$/i && vbecm.showVbsCommand && !explorerResourceIsFolder",
					"command": "explorer.import",
					"group": "myGroup@2"
				},
				{
					"when": "resourceExtname =~ /\\.xlsm$|\\.xlam$/i && vbecm.showVbsCommand && !explorerResourceIsFolder",
					"command": "explorer.compile",
					"group": "myGroup@3"
				},
				{
					"when": "resourceExtname =~ /\\.xlsm$|\\.xlam$/i && vbecm.showVbsCommand  && !explorerResourceIsFolder",
					"command": "explorer.exportFrx",
					"group": "myGroup@4"
				},
				{
					"when": "resourceExtname =~ /\\.xlsm$|\\.xlam$/i && vbecm.showVbsCommand  && explorerResourceIsFolder",
					"command": "explorer.commit-all",
					"group": "myGroup@5"
				},
        {
					"when": "resourceExtname =~ /\\.xlsm$|\\.xlam$/i && vbecm.showVbsCommand  && explorerResourceIsFolder",
					"command": "explorer.testModified",
					"group": "myGroup@6"
				}
			],
      "view/title": [
				{
					"command": "vbeDiffView.refreshEntry",
					"when": "view == vbeDiffView",
					"group": "navigation@1"
				},
        {
					"command": "vbeDiffView.collapseAll",
					"when": "view == vbeDiffView",
					"group": "navigation@2"
				}
      ],
      "view/item/context": [
        {
          "command": "vbeDiffView.diffVbeSrc",
          "when": "view == vbeDiffView && viewItem == FileDiffTreeItemBaseVbe"
        },
        {
          "command": "vbeDiffView.pushSrc",
          "when": "view == vbeDiffView && viewItem == FileDiffTreeItemBaseSrc"
        },
        {
          "command": "vbeDiffView.resolveVbe",
          "when": "view == vbeDiffView && viewItem == FileDiffTreeItemBaseVbe"
        }
      ],
			"editor/context": [
				{
					"when": "resourceExtname =~ /\\.bas$|\\.cls$|\\.frm$/i && vbecm.showVbsCommand",
					"command": "editor.run",
					"group": "myGroup@1"
				},
				{
					"when": "resourceExtname =~ /\\.bas$|\\.cls$|\\.frm$/i && vbecm.showVbsCommand",
					"command": "editor.commit",
					"group": "myGroup@2"
				},
				{
					"when": "resourceExtname =~ /\\.bas$|\\.cls$|\\.frm$/i && vbecm.showVbsCommand",
					"command": "editor.checkout",
					"group": "myGroup@3"
				}
			]
		},
		"configuration": {
			"title": "vbecm",
			"properties": {
				"vbecm.useSheetModule": {
					"type": "boolean",
					"default": true,
					"description": "Enable export sheet Modules(.sht.cls).Now always true.",
					"order": 1
				},
				"vbecm.vbsEncode": {
					"type": "string",
					"default": "windows-31j",
					"description": "Set system encoding. If Japanese, windows-31j",
					"order": 2
				}
			}
		}
	},
	"scripts": {
		"precompile": "rimraf out",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/runTest.js",
		"do-test": "node ./out/runTest.js",
		"vscode:prepublish": "npm run esbuild-base -- --minify",
		"esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
		"esbuild": "npm run esbuild-base -- --sourcemap",
		"esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
		"test-compile": "tsc -p ./",
		"prepackage": "npm run pretest",
		"package": "vsce package --target win32-x64"
	},
	"devDependencies": {
		"@types/chai": "^4.3.3",
		"@types/encoding-japanese": "^2.0.0",
		"@types/fs-extra": "^9.0.13",
		"@types/glob": "^7.2.0",
		"@types/iconv-lite": "^0.0.1",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/tar": "^6.1.2",
		"@types/vscode": "^1.69.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"@vscode/test-electron": "^2.1.5",
		"esbuild": "^0.14.50",
		"eslint": "^8.18.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"@types/uuid": "^8.3.4",
		"chai": "^4.3.6",
		"dir-compare": "^4.0.0",
		"encoding-japanese": "^2.0.0",
		"fs-extra": "^10.1.0",
		"fstream": "^1.0.12",
		"iconv-lite": "^0.6.3",
		"tar": "^6.1.11",
		"uuid": "^9.0.0"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/toramameseven/vbe-client-mini.git"
	},
	"homepage": "https://github.com/toramameseven/vbe-client-mini",
	"__metadata": {
		"id": "51bfa7a5-c56a-47e5-a9e4-616b13e817e5",
		"publisherDisplayName": "toramameseven",
		"publisherId": "90488179-4dd6-4c34-99d7-db81d61b6862",
		"isPreReleaseVersion": false
	}
}
